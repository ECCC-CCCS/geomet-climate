// mapserver template
{
[resultset layer={}]
  "type": "FeatureCollection",
  "features": [
    [feature trimlast=","]
    {
      "type": "Feature",
      "id": "[_id]",
      "geometry": {
        "type": "Point",
        "coordinates":
          [shpxy precision="2" xh="[" yf="]"]
      },
      "properties": {
        "CONTRIBUTOR_EN": "[CONTRIBUTOR_EN]",
        "CONTRIBUTOR_FR": "[CONTRIBUTOR_FR]",
        "IDENTIFIER": "[IDENTIFIER]",
        "PROV_TERR_STATE_LOC": "[PROV_TERR_STATE_LOC]",
        "STATION_NAME": "[STATION_NAME]",
        "STATION_NUMBER": "[STATION_NUMBER]",
        "STATUS_EN": "[STATUS_EN]",
        "STATUS_FR": "[STATUS_FR]",
        "VERTICAL_DATUM": "[VERTICAL_DATUM]"
      }
    },
    [/feature]
  ]
[/resultset]
}
