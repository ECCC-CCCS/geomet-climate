// mapserver template
{
[resultset layer={}]
  "type": "FeatureCollection",
  "features": [
    [feature trimlast=","]
    {
      "type": "Feature",
      "id": "[_id]",
      "geometry": {
        "type": "Point",
        "coordinates": 
          [shpxy precision="2" xh="[" yf="]"]
      },
      "properties": {
        "elevation__elevation": "[elevation__elevation]",
        "identifier__identifiant": "[identifier__identifiant]",
        "joined__rejoint": "[joined__rejoint]",
        "measurement_type__type_mesure": "[measurement_type__type_mesure]",
        "period__periode": "[period__periode]",
        "province__province": "[province__province]",
        "station_id__id_station": "[station_id__id_station]",
        "station_name__nom_station": "[station_name__nom_station]",
        "trend_value__valeur_tendance": "[trend_value__valeur_tendance]",
        "year_range__annees": "[year_range__annees]"
      }
    },
    [/feature]
  ]
[/resultset]
}
