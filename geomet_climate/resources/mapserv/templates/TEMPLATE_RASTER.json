// mapserver template
{
[resultset layer={}]
  "type": "FeatureCollection",
  "layer": "[layer]",
  "features": [
    [feature trimlast=","]
    {
      "type": "Feature",
      "id": "[x]/[y]",
      "geometry": {
        "type": "Point",
        "coordinates": [
            [x],
            [y]
        ]
      },
      "properties": {
        "value": "[value_0]"
      }
    },
    [/feature]
  ]
[/resultset]
}
