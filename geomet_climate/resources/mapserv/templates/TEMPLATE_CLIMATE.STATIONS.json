// mapserver template
{
[resultset layer={}]
  "type": "FeatureCollection",
  "features": [
    [feature trimlast=","]
    {
      "type": "Feature",
      "id": "[_id]",
      "geometry": {
        "type": "Point",
        "coordinates":
          [shpxy precision="2" xh="[" yf="]"]
      },
      "properties": {
        "CLIMATE_IDENTIFIER": "[CLIMATE_IDENTIFIER]",
        "COUNTRY": "[COUNTRY]",
        "DISPLAY_CODE": "[DISPLAY_CODE]",
        "ELEVATION": "[ELEVATION]",
        "ENG_PROV_NAME": "[ENG_PROV_NAME]",
        "ENG_STN_OPERATOR_ACRONYM": "[ENG_STN_OPERATOR_ACRONYM]",
        "ENG_STN_OPERATOR_NAME": "[ENG_STN_OPERATOR_NAME]",
        "FRE_PROV_NAME": "[FRE_PROV_NAME]",
        "FRE_STN_OPERATOR_ACRONYM": "[FRE_STN_OPERATOR_ACRONYM]",
        "FRE_STN_OPERATOR_NAME": "[FRE_STN_OPERATOR_NAME]",
        "HAS_MONTHLY_SUMMARY": "[HAS_MONTHLY_SUMMARY]",
        "HAS_NORMALS_DATA": "[HAS_NORMALS_DATA]",
        "LATITUDE": "[LATITUDE]",
        "LONGITUDE": "[LONGITUDE]",
        "NORMAL_CODE": "[NORMAL_CODE]",
        "PROV_STATE_TERR_CODE": "[PROV_STATE_TERR_CODE]",
        "PUBLICATION_CODE": "[PUBLICATION_CODE]",
        "STATION_NAME": "[STATION_NAME]",
        "STATION_TYPE": "[STATION_TYPE]",
        "STN_ID": "[STN_ID]",
        "TC_IDENTIFIER": "[TC_IDENTIFIER]",
        "TIMEZONE": "[TIMEZONE]",
        "WMO_IDENTIFIER": "[WMO_IDENTIFIER]"
      }
    },
    [/feature]
  ]
[/resultset]
}
